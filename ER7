    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         

        int A;
        int B;
        
        A = Integer.parseInt(txtA.getText());
        B = Integer.parseInt(txtB.getText());
        
        
        if (A > B){
            txtR.setText(String.valueOf("El valor de A es mayor que B"));
        }
            else if (A == B){
                txtR.setText(String.valueOf("El valor de A es igual que B"));
            }
                else if ( B > A){
                    txtR.setText(String.valueOf("El valor de A es menor que B"));
        }
                
   
    }                                        

