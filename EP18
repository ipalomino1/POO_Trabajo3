    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
    
    double COD;
    String NOM;
    double HM;
    double VT;
    double PR;
    double SB;
    double SR;
    
    COD = Double.parseDouble(txtCOD.getText());
    NOM = (txtNOM.getText());
    HM = Double.parseDouble(txtHM.getText());
    VT = Double.parseDouble(txtVT.getText());
    PR = Double.parseDouble(txtPR.getText());
    
    SB = HM*VT;
        
    SR = SB*(1-(PR/100));
    
    
    txtSB.setText(String.valueOf(SB));
    txtSR.setText(String.valueOf(SR));   
    
    
    


        
    }                                        

    private void txtBorrarActionPerformed(java.awt.event.ActionEvent evt) {                                          

    txtCOD.setText("");
    txtNOM.setText("");
    txtHM.setText("");
    txtVT.setText("");
    txtPR.setText("");
    txtSB.setText("");
    txtSR.setText("");  
    
    
    
    }                                         

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         


    System.exit(0);

        // TODO add your handling code here:
    }  
